{
  "backup_data_uid": "6a26422187fdfc33", 
  "data_name": "axbc-snap", 
  "no_compile": "yes", 
  "no_target_file": "yes", 
  "print_files_after_run": [
    "stderr.log", 
    "stdout.log"
  ], 
  "process_in_tmp": "yes", 
  "program": "yes", 
  "run_cmds": {
    "default": {
      "ignore_return_code": "no", 
      "run_time": {
        "fine_grain_timer_file": "tmp-ck-timer.json", 
        "post_process_via_ck": "yes", 
        "run_cmd_main": "mpirun -np $<<CK_MPI_PROCESS>>$ $<<CK_ENV_AXBC_BIN>>$ -f $<<CK_ENV_DATASET_SNAP>>$/$<<CK_GRAPH_NAME>>$ -p 1x1 -n $<<CK_NUM_VERTICES>>$ -N 100 -c 0 -x 1 -d 0 -H 0 -o $<<CK_GRAPH_NAME>>$", 
        "run_cmd_out1": "stdout.log", 
        "run_cmd_out2": "stderr.log", 
        "run_output_files": [
          "stdout.log", 
          "stderr.log", 
          "tmp-ck-timer.json"
        ]
      }
    }
  }, 
  "run_deps": {
    "lib-axbc": {
      "local": "yes", 
      "name": "Approximating BC", 
      "sort": 10, 
      "tags": "lib,abc,bc,approximation"
    },
    "dataset-snap": {
      "local": "yes", 
      "name": "Snap dataset", 
      "sort": 10, 
      "tags": "dataset,graph,snap"
    }
  }, 
  "run_vars": {
    "CK_MPI_PROCESS": 1, 
    "CK_GRAPH_NAME":"facebook_combined.txt",
    "CK_NUM_VERTICES": 4039
  }, 
  "tags": [
    "graph"
  ]
}
