{
  "backup_data_uid": "079c1ff2d6432f96", 
  "data_name": "seissol-demo", 
  "no_compile": "yes", 
  "no_target_file": "yes", 
  "print_files_after_run": [
    "stderr.log", 
    "stdout.log"
  ], 
  "process_in_tmp": "yes", 
  "program": "yes", 
  "run_cmds": {
    "default": {
      "ignore_return_code": "no", 
      "run_time": {
        "fine_grain_timer_file": "tmp-ck-timer.json", 
        "post_process_via_ck": "yes", 
        "run_cmd_main": "$<<CK_ENV_LIB_MPI_BIN>>$/mpirun -np $<<CK_MPI_PROCESS>>$ $<<CK_ENV_SEISSOL_BIN_1>>$ -h", 
        "run_cmd_out1": "stdout.log", 
        "run_cmd_out2": "stderr.log", 
        "run_output_files": [
          "stdout.log", 
          "stderr.log", 
          "tmp-ck-timer.json"
        ]
      }
    }
  }, 
  "run_deps": {
    "lib-seissol": {
      "local": "yes", 
      "name": "SeisSol Library", 
      "sort": 10, 
      "tags": "lib,seissol"
    }
  }, 
  "run_vars": {
    "CK_MPI_PROCESS": 1 
  }, 
  "tags": [
    "scientific",
    "cluster",
    "cluster-competition"
  ]
}
